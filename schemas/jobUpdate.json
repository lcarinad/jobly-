{
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": ["title", "salary", "equity"],
  "properties": {
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["Manager"]
    },
    "salary": {
      "type": "integer",
      "default": 0,
      "title": "The salary Schema",
      "examples": [100000]
    },
    "equity": {
      "type": "number",
      "default": 0.0,
      "title": "The equity Schema",
      "examples": [0.5]
    }
  },
  "examples": [
    {
      "title": "Manager",
      "salary": 100000,
      "equity": 0.5
    }
  ]
}
